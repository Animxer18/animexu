{
  "name": "animex-client",
  "private": true,
  "version": "0.0.0",
  "description": "Front-end source code for Animex, a free anime streaming website made with React, SASS,GraphQL, and TypeScript.",
  "scripts": {
    "full-dev": "concurrently \"npm:dev\" \"cd ../server/src && npx tsc --watch \" \"cd ../server/dist && npx nodemon index.js\"",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohitman47/animex-client.git"
  },
  "keywords": [
    "anime",
    "streaming",
    "typescript",
    "graphql"
  ],
  "bugs": {
    "url": "https://github.com/rohitman47/animex-client/issues"
  },
  "homepage": "https://github.com/rohitman47/animex-client#readme",
  "author": "rohitman47",
  "license": "MIT",
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "concurrently": "^7.2.2",
    "postcss": "^8.4.14",
    "postcss-preset-env": "^7.7.2",
    "sass": "^1.53.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-plugin-svgr": "^2.2.0"
  }
