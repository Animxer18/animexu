{
  "name": "animex",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Source code for Animex, a free anime streaming website made with react, typescript, graphql, and express.",
  "scripts": {
    "dev": "concurrently \"cd ./client && vite\" \"cd ./server/src && npx tsc --watch \" \"cd ./server/dist && npx nodemon index.js\"",
    "build:client": "cd ./client && tsc && vite build",
    "build": "cd ./client && npm run build && cd ./server && npx tsc",
    "preview": "cd ./client && vite preview",
    "start": "cd ./server/dist && node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohitman47/animex.git"
  },
  "keywords": [
    "anime",
    "streaming",
    "typescript",
    "graphql"
  ],
  "bugs": {
    "url": "https://github.com/rohitman47/animex/issues"
  },
  "homepage": "https://github.com/rohitman47/animex#readme",
  "author": "rohitman47",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.18",
    "postcss": "^8.4.14",
    "postcss-preset-env": "^7.7.2",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vite-plugin-svgr": "^2.2.0"
  }
}
