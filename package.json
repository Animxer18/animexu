{
  "name": "animex",
  "private": true,
  "version": "0.0.0",
  "description": "Source code for Animex, a free anime streaming website made with react, sass, typescript, graphql, and express.",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"cd ./client && vite\" \"cd ./server && tsc --watch \" \"cd ./server && tsc && cd ./dist && nodemon index.js\"",
    "build": "concurrently \"cd ./client && tsc && vite build\" \"cd ./server && tsc\"",
    "start": "cd ./server/dist && node index.js",
    "preview": "npm run build && npm run start",
    "preview-client": "cd ./client && vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohitman47/animex.git"
  },
  "keywords": [
    "anime",
    "streaming",
    "typescript",
    "graphql"
  ],
  "bugs": {
    "url": "https://github.com/rohitman47/animex/issues"
  },
  "homepage": "https://github.com/rohitman47/animex#readme",
  "author": "rohitman47",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.2",
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.18",
    "postcss": "^8.4.14",
    "postcss-preset-env": "^7.7.2",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vite-plugin-svgr": "^2.2.0"
  }
}
